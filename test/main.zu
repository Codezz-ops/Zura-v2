
fn addTwoArrays(x, y)   { return x + y; }
fn popFromArray(x, y)   { return x <- y; }
fn pushToArray(x, y, z) { return x -> y @ z; }

have x := [1, 3, 2];
info addTwoArrays(x, [2, 4]); // [1, 3, 2, 2, 4]
info popFromArray(x, 1);      // [1, 2]
info pushToArray(x, 4, 1);    // [1, 4, 2]


